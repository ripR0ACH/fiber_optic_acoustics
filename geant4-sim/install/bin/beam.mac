#----- Set the beam energy profile -----
/gps/particle opticalphoton
/gps/ene/type Gauss
/gps/ene/mono 4.75 eV
/gps/ene/sigma 10 eV
/gps/ene/min 1.9 eV
/gps/ene/max 7.6 eV
/gps/ene/gradient -200 MeV
/gps/polarization 0 0 0

#----- Set the beam geometry -----

# Circular beam source. Simulates a single fiber optic
/gps/pos/type Beam
/gps/pos/shape Circle
/gps/pos/radius 1.5 mm
# /gps/pos/radius 1.5 mm
# /gps/pos/sigma_r 0.002 mm

# Rectangular plane for infinite resolution
# /gps/pos/type Plane
# /gps/pos/shape Rectangle
# /gps/pos/halfx 18 mm #28.5 square for sim val
# /gps/pos/halfy 5 mm

# the incident surface is in the x-z plane just below the gas
/gps/pos/rot1 0 1 0
/gps/pos/rot2 0 0 1
/gps/pos/centre 0. 26 30 mm

# the photons are emitted around the y-axis
# /gps/ang/rot1 0 1 0
# /gps/ang/rot2 -1 0 0
/gps/ang/type focused #change to focus to focus to a point with /gps/ang/focuspoint point coords mm
/gps/ang/focuspoint 0 6 0 mm
# /gps/direction 0 -0.5547 -0.8321 #45ish

#----- Set beam angular dispersion for fiber optic -----
# N.A. is 0.22 so acceptance cone is ~25.4 degrees
#/gps/ang/sigma_r 25.40 deg

## collimated light for simulation-experiment matching
##/gps/ang/sigma_r 5 deg #radial standard deviation
# /gps/ang/maxtheta 6 deg #maximum theta value
# /gps/ang/sigma_r 2 deg


#----- Set beam angular dispersion from histogram (Cherenkov)-----
# here, hist.mac holds a cherenkov light distribution (generated thorugh simulation). Setting to true will use this distribution. Straight light is the default, unless otherwise specified.
# /gps/ang/surfnorm false
# /gps/ang/type user
# /control/execute hist.mac

